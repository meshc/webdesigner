<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.nayaware.webdesigner.editors.MultiPageEditor"
            contributorClass="com.nayaware.webdesigner.editors.MultiPageEditorContributor"
            extensions="html"
            icon="icons/sample.gif"
            id="com.nayaware.webdesigner.editors.MultiPageEditor"
            name="Web Designer">
      </editor>
      <editor
            class="com.nayaware.webdesigner.editors.html.HTMLEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            extensions="html"
            icon="icons/sample.gif"
            id="com.nayaware.webdesigner.editors.HTMLEditor"
            name="HTML Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="com.nayaware.webdesigner.palette.PaletteView"
            icon="icons/palette.png"
            id="com.nayaware.webdesigner.palette.PaletteView"
            name="Palette">
      </view>
   </extension>

</plugin>
